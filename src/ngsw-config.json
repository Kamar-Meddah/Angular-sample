{
    "index": "/index.html",
    "assetGroups": [{
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.json"
        ],
        "versionedFiles": [
          "/*.bundle.css",
          "/*.bundle.js",
          "/*.chunk.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/assets/icons/**",
          "/*.eot",
          "/*.svg",
          "/*.woff2",
          "/*.ttf",
          "/*.woff"
        ]
      }
    }],
    "dataGroups": [
        {
            "name":"test",
            "urls":[
                "http://localhost/img/articles/*.jpg",

                "http://localhost/?request=Categories.all",
                "http://localhost/?request=Categories.index&page",

                "http://localhost/?request=Images.find&id",

                "http://localhost/?request=Articles.index&page",
                "http://localhost/?request=Articles.show&id",
                "http://localhost/?request=Articles.byCategorie&category_id&page",
                "http://localhost/?request=Articles.search&search&page",

                "http://localhost/?request=Comments.find&id"
                
            ],
            "cacheConfig":{
                "maxSize": 100,
                "maxAge": "1h",
                "timeout": "10s",
                "strategy": "freshness"

            }
        }
    ]
  }